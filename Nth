 // Nth node from end of linked list
 
 /*
 // information about the class Node
 class Node
 {
     int data;
     Node next;
     Node(int d) 
     {
         data = d; 
         next = null;
     }
 }
 */
 static int getNthFromLast(Node head, int n)
 {
 
        // Your code here
        int count = 0;
        Node temp = head;
        while(temp!=null){
            count++;
            temp = temp.next;
        }
 
        int k = count-n;
 
        if(k<0){
            return -1;
        }
 
         temp = head;
        for(int i=0; i<k; i++){
            temp = temp.next;
        }
 
        return temp.data;
 }
